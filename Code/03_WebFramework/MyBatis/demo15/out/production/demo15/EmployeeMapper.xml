<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.mio4.mapper.EmployeeMapper">
    <select id="selectArticleByOrderId" parameterType="int" resultType="com.mio4.domain.Article">
        SELECT * FROM TB_ARTICLE WHERE id IN (SELECT article_id FROM TB_ITEM WHERE order_id = #{id})
    </select>
</mapper>